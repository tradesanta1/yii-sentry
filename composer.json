{
  "name": "yii2-sentry2",
  "description": "Yii2 logger for Sentry with SDK version 2",
  "keywords": [
    "sentry",
    "sentry2",
    "yii2"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "process-timeout": 1800
  },
  "require": {
    "yiisoft/yii2": "~2.0",
    "sentry/sdk": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "yii2sentry2\\sentry\\": "src"
    }
  }
}
